{
  "version": 2,
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/products",
      "methods": ["GET", "POST"],
      "headers": {
        "Access-Control-Allow-Origin": "https://jasmin-shopy.web.app",
        "Access-Control-Allow-Headers": "Content-Type",
        "Access-Control-Allow-Methods": "GET, POST , PUT , DELETE"
      }
    }
  ]
}
